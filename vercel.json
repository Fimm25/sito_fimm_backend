{
    "version": 2,
    "builds": [
      {
        "src": "frontend/build/**/*",
        "use": "@vercel/static"
      },
      {
        "src": "index.js",
        "use": "@vercel/node"
      }
    ],
    "routes": [
      {
        "src": "/(.*)",
        "dest": "/frontend/build/index.html"
      },
      {
        "src": "/api/(.*)",
        "dest": "/api/$1"
      }
    ]
  }
  