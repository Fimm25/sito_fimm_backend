

    {
        "version": 2,
        "builds": [
          {
            "src": "frontend/dist/**/*",
            "use": "@vercel/static"
          },
          {
            "src": "index.js",
            "use": "@vercel/node"
          }
        ],
        "routes": [
          {
            "src": "/(.*)",
            "dest": "/frontend/dist/index.html"
          },
          {
            "src": "/api/(.*)",
            "dest": "/api/$1"
          }
        ]
      }
      